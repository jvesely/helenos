interface pci extends service {
		/* Establish connection */
		ipcarg_t ipc_m_connect_me_to(void);
		
		/* Close connection */
		ipcarg_t ipc_m_phone_hungup(void);
	protocol:
		[pci.bp]
};

frame pci {
	provides:
		pci pci;
	requires:
		[/uspace/lib/libc/requires]
		ns ns;
	protocol:
		[/uspace/lib/libc/protocol] +
		[pci_server.bp]
};
